        "import re\n",
        "#(ab)*+ (a+b) + (ba)*\n",
        "#pattern = '^[ab]*|[a|b]|[ba]*$'\n",
        "pattern = re.compile('a*')\n",
        "test_string = 'bbb'\n",
        "result = re.match(pattern, test_string)\n",
        "\n",
        "if result:\n",
        "  print(\"Search successful.\")\n",
        "else:\n",
        "  print(\"Search unsuccessful.\")\t\n"
